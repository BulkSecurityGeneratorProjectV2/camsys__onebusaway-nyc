<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
   http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">
	<context:component-scan base-package="org.onebusaway.nyc.transit_data_manager.api" />
	
	<bean class="org.onebusaway.container.spring.SystemPropertyOverrideConfigurer">
        <property name="order" value="-2" />
        <property name="properties">
            <props>
                <prop key="dataPath">d:\</prop>
                <prop key="crewAssignFilename">CrewAssignSample.csv</prop>
                <prop key="depotAssignFilename">DOB_draft_depot_assignments.xml</prop>
                <prop key="pipoFilename">CrewAssignSample.csv</prop>
                <prop key="dscFilename">dsc.csv</prop>
                <prop key="prettyPrintOutput">true</prop>
            </props>
        </property>
    </bean>

	
</beans>