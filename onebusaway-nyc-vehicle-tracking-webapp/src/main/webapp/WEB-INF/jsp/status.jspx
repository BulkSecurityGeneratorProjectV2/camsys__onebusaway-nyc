<html xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:c="http://java.sun.com/jsp/jstl/core">
<jsp:directive.page contentType="text/html" />
<head>
<title>Status</title>
</head>
<body>
	<p>
		<b>ListenerTask:</b> <c:out value="${sm.listenerTask}" />
		<br/>
		<b>OutputService:</b> <c:out value="${sm.outputService}" />
		<br/>
		<b>HostName:</b> <c:out value="${sm.hostname}" />
		<br/>
		<b>Depots:</b> <c:out value="${sm.depotList}" />
		<br/>
		<b>IsPrimary?:</b> <c:out value="${sm.primary}" />
		<br/>
		<b>Git Version:</b> <c:out value="${sm.gitDescribe}" />
		<br/>
		<b>Instance Id:</b> <c:out value="${sm.instanceId}" />
		<br/>
		<b>Instance Type:</b> <c:out value="${sm.instanceType}" />
		<br/>
		<b>AMI Id:</b> <c:out value="${sm.amiId}" />
		<br/>
		<b>Public Hostname:</b> <c:out value="${sm.publicHostname}" />
		<br/>
		<b>Internal Hostname:</b> <c:out value="${sm.internalHostname}" />
		<br/>

	</p>
</body>
</html>
